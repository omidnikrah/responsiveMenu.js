{
	"extends": "airbnb",
	"rules": {
	  "react/jsx-quotes": 0,
	  "jsx-quotes": [2, "prefer-double"],
	  "comma-dangle": [2, "never"],
	  "max-len": 0,
	  "react/prop-types": 0,
	  "class-methods-use-this": 0,
	  "consistent-return": 0,
	  "jsx-a11y/href-no-hash": "off",
	  "rules":{
			"indent": [2, "tab"],
			"no-tabs": 0
	  }
	},
	"parser": "babel-eslint"
  }